<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @feed.name %>
</p>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Cover image url</th>
      <th>Author name</th>
      <th>Audio url</th>
      <th>Feed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.title %></td>
        <td><%= item.description %></td>
        <td><%= item.cover_image_url %></td>
        <td><%= item.author_name %></td>
        <td><%= item.audio_url %></td>
        <td><%= item.feed %></td>        
      </tr>
    <% end %>
  </tbody>
</table>